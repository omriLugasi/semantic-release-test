{
  "name": "sr-test",
  "version": "1.1.0",
  "private": true,
  "main": "index_2.js",
  "repository": "git@github.com:omriLugasi/semantic-release-test.git",
  "author": "omri <omlrc1991@gmail.com>",
  "license": "MIT",
  "scripts": {
    "publish-new-tag": "CI=true GH_TOKEN=<TOKEN> ./node_modules/.bin/semantic-release",
    "commit": "cz"
  },
  "dependencies": {
    "semantic-release": "^17.4.3"
  },
  "release": {
    "branches": [
      "master",
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "Docs",
              "release": "patch"
            },
            {
              "type": "tests",
              "scope": "Test",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "debug": true
  }
}
